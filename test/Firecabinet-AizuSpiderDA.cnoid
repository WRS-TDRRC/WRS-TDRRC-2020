                baseDirectory: "Controller directory"
                reloading: true
                isOldTargetVariableMode: false
              children: 
                - 
                  id: 8
                  name: "Arm1Controller"
                  plugin: Body
                  class: SimpleControllerItem
                  data: 
                    isNoDelayMode: false
                    controllerOptions: "ARM1_"
                    controller: "Jaco2Controller.so"
                    baseDirectory: "Controller directory"
